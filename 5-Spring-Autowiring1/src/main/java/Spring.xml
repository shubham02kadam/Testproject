<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context.xsd">

      <context:annotation-config></context:annotation-config>
      
        <bean id="address" class="com.jdc.Address">
        <property name="landmark" value="Near akloi"></property>
        <property name="city" value="basmath"></property>
        <property name="country" value="India"></property>
        </bean>
        
       <!--<bean id="employee" class="com.jdc.Employee" autowire="byName"></bean> -->
        
       <bean id="employee" class="com.jdc.Employee"></bean>
        
        
        
        
       
        
       </beans>


   

